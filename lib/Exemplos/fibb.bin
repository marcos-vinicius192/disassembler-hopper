    MOV R0, #0        ; F0
    MOV R1, #1        ; F1
    MOV R3, #5        ; número de iterações
    MOV R7, #0xFFFF   ; -1 para decrementar R3
LOOP:
    MOV R2, R0        ; R2 = R0
    ADD R2, R1        ; R2 = R0 + R1  (novo termo)
    MOV R0, R1        ; desloca: R0 <- antigo R1
    MOV R1, R2        ; desloca: R1 <- novo termo
    ADD R3, R7        ; R3 -= 1
    JZ END
    JMP LOOP
END:
    HLT
